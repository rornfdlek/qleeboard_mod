{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "standard",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "quotes": ["error", "single"],
        "no-var":"error",
        "no-console": "off",
        "no-unused-vars":"off",
        "no-alert": "off"
    }
}
